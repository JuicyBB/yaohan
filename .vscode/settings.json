{
  "liveServer.settings.port": 5501,
  "liveServer.settings.ignoreFiles": [
    ".vscode/**",
    "**/*.scss",
    "**/*.sass",
    // "**/*.js",
    // "**/*.css",
    // "**/*.nunjucks",
    "**/*.ts"
  ],
  "liveServer.settings.wait": 2000,
  "liveServer.settings.root": "/docs",
  "files.associations": {
    "*.nunjucks": "html"
  },
  "livePreview.defaultPreviewPath": "/docs/index.html"
}
